PROJECT_NAME = "DevTools"
PROJECT_NUMBER = @CMAKE_PROJECT_VERSION@
PROJECT_BRIEF = "New Development Tools"
# ビルドディレクトリからの相対パス
PROJECT_ICON = "docs/dev-tools_icon.doxygen.png"
# ビルドディレクトリからの相対パス
INPUT = @CMAKE_CURRENT_SOURCE_DIR@/docs/ @CMAKE_CURRENT_SOURCE_DIR@/core/ @CMAKE_CURRENT_SOURCE_DIR@/gui/ 
FILE_PATTERNS = *.cpp *.h *.dox *.md
RECURSIVE = YES
OUTPUT_DIRECTORY = doxygen
#EXTRACT_ALL = YES
STRIP_FROM_PATH = @CMAKE_CURRENT_SOURCE_DIR@

# privateメンバーを含める
EXTRACT_PRIVATE = YES

# 言語
OUTPUT_LANGUAGE = Japanese-en

# includeの書き方を #include "file" にする
FORCE_LOCAL_INCLUDES = YES

# LaTeXを無効化
GENERATE_LATEX = NO

# MathJaxを有効化
USE_MATHJAX = YES
# MathJax_2の場合
#MATHJAX_VERSION = MathJax_2
#MATHJAX_EXTENSIONS = TeX/AMSmath TeX/AMSsymbols
# MathJax_3の場合
MATHJAX_VERSION = MathJax_3
MATHJAX_EXTENSIONS = ams
HTML_FORMULA_FORMAT = svg

# ソースコードを含めない
VERBATIM_HEADERS = NO
SOURCE_BROWSER = NO
REFERENCES_LINK_SOURCE = NO

# プリプロセッサを動かす
ENABLE_PREPROCESSING = YES
MACRO_EXPANSION = YES
INCLUDE_PATH = @CMAKE_CURRENT_SOURCE_DIR@/
